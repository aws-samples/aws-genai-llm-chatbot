{
  "name": "aws-genai-llm-chatbot",
  "version": "3.1.3",
  "bin": {
    "aws-genai-llm-chatbot": "bin/aws-genai-llm-chatbot.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "cdk": "cdk",
    "create": "node ./cli/magic.js create",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "npx prettier --ignore-path .gitignore --write \"**/*.+(js|ts|jsx|tsx|json|css)\"",
    "deploy": "npm run format && npx cdk deploy"
  },
  "devDependencies": {
    "@aws-lambda-powertools/batch": "^1.14.1",
    "@aws-lambda-powertools/logger": "^1.14.1",
    "@aws-sdk/credential-provider-node": "^3.441.0",
    "@aws-sdk/protocol-http": "^3.374.0",
    "@aws-sdk/signature-v4": "^3.374.0",
    "@types/aws-lambda": "^8.10.125",
    "@types/node": "20.1.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "aws-cdk": "2.104.0",
    "eslint": "^8.45.0",
    "prettier": "^3.0.3",
    "ts-node": "^10.9.1",
    "typescript": "~5.1.3"
  },
  "dependencies": {
    "@aws-cdk/aws-apigatewayv2-alpha": "^2.104.0-alpha.0",
    "@aws-cdk/aws-apigatewayv2-authorizers-alpha": "^2.104.0-alpha.0",
    "@aws-cdk/aws-apigatewayv2-integrations-alpha": "^2.104.0-alpha.0",
    "@aws-cdk/aws-cognito-identitypool-alpha": "^2.104.0-alpha.0",
    "aws-cdk-lib": "2.104.0",
    "commander": "^11.0.0",
    "constructs": "^10.0.0",
    "enquirer": "^2.4.1",
    "source-map-support": "^0.5.21"
  }
}
